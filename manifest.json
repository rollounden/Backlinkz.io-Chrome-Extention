{
  "manifest_version": 3,
  "name": "SEO Page Analyzer by Apex Marketing",
  "version": "1.2",
  "description": "Analyze SEO metrics for any webpage",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
