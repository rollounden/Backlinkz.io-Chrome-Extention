{
  "manifest_version": 3,
  "name": "Backlinzk.io: Making SEO Easy ",
  "version": "1.1",
  "description": "Analyze SEO signals, write better code and analyse local business information for any webpage",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
